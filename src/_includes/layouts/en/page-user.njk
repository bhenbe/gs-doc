---
title: TheGreenShot Documentation
lang: en
---
<!doctype html>
<html lang="{{ locale }}" dir="{{ dir }}">
  <head>
    {% include '../../head.njk' %}
  </head>
  <body>
    {% include '../../header.njk' %}
    <main class="max-w-prose mx-auto p-4">
      <h1 class="text-2xl mb-3">{{ title }}</h1>
      <div class="prose">
        {{ content | safe }}
      </div>
      <aside class="my-20">
        <h2 class="text-2xl mb-3 text-gray-600">{{ 'RelatedPosts' | i18n }}</h2>
          <ul>
          {%- for post in collections.userEn | sortByOrder -%}
            {% if page.url != post.url %}
            <li class="mb-2"><a href="{{ post.url }}" class="text-blue-600 underline">{{ post.data.title }}</a></li>
            {% endif %}
          {%- endfor -%}
            <li class="mb-2"><a href="{{ '/en/' | url }}" class="text-blue-600 underline">{{ 'BackToHome' | i18n }}</a></li>
          </ul>
      </aside>
    </main>
    {% include '../../footer.njk' %}
  </body>
</html>